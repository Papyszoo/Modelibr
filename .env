# Web API
ASPNETCORE_ENVIRONMENT=Development
WEBAPI_HTTP_PORT=8080
WEBAPI_HTTPS_PORT=8081

# Frontend
FRONTEND_PORT=3000
VITE_API_BASE_URL=https://localhost:8081

# SQL Server
ACCEPT_EULA=Y
SA_PASSWORD=ChangeThisStrongPassword123!
MSSQL_PID=Developer
MSSQL_PORT=1433

# Thumbnail Worker Service
WORKER_PORT=3001
WORKER_ID=worker-1
POLL_INTERVAL_MS=5000
MAX_CONCURRENT_JOBS=3
RENDER_WIDTH=256
RENDER_HEIGHT=256
RENDER_FORMAT=png
LOG_LEVEL=info

# Thumbnail Storage
THUMBNAIL_STORAGE_ENABLED=true
THUMBNAIL_STORAGE_PATH=/var/lib/modelibr/thumbnails
SKIP_DUPLICATE_THUMBNAILS=true

# Docker (optional)
DOCKER_REGISTRY=
HOST_UID=1000
HOST_GID=1000
